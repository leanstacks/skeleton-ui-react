    "@tanstack/react-table": "8.17.3",
    "axios": "1.7.2",
    "classnames": "2.5.1",
    "dayjs": "1.11.11",
    "formik": "2.4.6",
    "i18next": "23.11.5",
    "i18next-browser-languagedetector": "8.0.0",
    "lodash": "4.17.21",
